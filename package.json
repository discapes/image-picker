    "svelte": "^3.0.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "express": "^4.18.1",
    "globby": "^13.1.1",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.12",
    "sirv-cli": "^2.0.0",
